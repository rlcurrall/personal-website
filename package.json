{
    "name": "personal-website",
    "version": "2.1.0",
    "private": true,
    "engines": {
        "node": "14.x"
    },
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "lint": "eslint ./src/**/*{.js,.vue}",
        "lint:fix": "eslint --fix ./src/**/*{.js,.vue}"
    },
    "dependencies": {
        "vue": "^3.0.4",
        "vue-router": "^4.0.0-0"
    },
    "devDependencies": {
        "@tailwindcss/postcss7-compat": "^2.0.2",
        "@vue/compiler-sfc": "^3.0.4",
        "@vue/eslint-config-prettier": "^6.0.0",
        "autoprefixer": "^9.8.6",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-vue": "^7.3.0",
        "postcss": "^7.0.36",
        "prettier": "^1.19.1",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
        "vite": "^1.0.0-rc.13"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended",
            "@vue/prettier"
        ]
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 120
    }
}
